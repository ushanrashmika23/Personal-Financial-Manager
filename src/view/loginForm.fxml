<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane fx:id="context" prefHeight="266.0" prefWidth="391.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Login">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <TextField fx:id="txtUsername" layoutX="14.0" layoutY="63.0" prefHeight="30.0" prefWidth="363.0" promptText="Username">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <PasswordField fx:id="txtPassword" layoutX="14.0" layoutY="113.0" prefHeight="30.0" prefWidth="363.0" promptText="Password">
         <font>
            <Font size="14.0" />
         </font>
      </PasswordField>
      <Button layoutX="14.0" layoutY="164.0" mnemonicParsing="false" onAction="#loginOnAction" prefHeight="40.0" prefWidth="363.0" text="Login" />
      <Label layoutX="110.0" layoutY="216.0" text="Don't have an account? Register" />
   </children>
</AnchorPane>
